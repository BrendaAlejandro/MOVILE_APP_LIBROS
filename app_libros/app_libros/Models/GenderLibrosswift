import Foundation

struct GenderLibros: Identifiable, Decodable {
    let id: Int
    let autor: String
    let title: String
    let descripcion: String
    let año: Double
    let genero: String
    let imageUrl: String
    var isFavorite: Bool = false
    
    enum CodingKeys: String, CodingKey {
        case id = "id"
        case autor = "name"
        case title = "brand"
        case descripcion = "gender"
        case año = "category"
        case genero = "image"
        case imageUrl = "price"
    }
}


